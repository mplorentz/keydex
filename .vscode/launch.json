{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Tests",
            "type": "dart",
            "request": "launch",
            "program": "test/",
            "args": [
                "--reporter",
                "expanded"
            ]
        },
        {
            "name": "Golden",
            "request": "launch",
            "type": "dart",
            "codeLens": {
                "for": [
                    "run-test",
                    "run-test-file"
                ]
            },
            "args": [
                "--update-goldens"
            ]
        }
    ]
}